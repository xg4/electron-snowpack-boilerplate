{
  "name": "electron-snowpack-boilerplate",
  "version": "0.0.0",
  "description": "electron snowpack boilerplate",
  "keywords": [
    "electron",
    "snowpack",
    "boilerplate",
    "react",
    "postcss",
    "tailwind",
    "typescript"
  ],
  "homepage": ".",
  "bugs": {
    "url": "https://github.com/xg4/electron-snowpack-boilerplate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xg4/electron-snowpack-boilerplate.git"
  },
  "license": "MIT",
  "author": "xg4 <xingor4@gmail.com>",
  "main": "electron/mod.js",
  "scripts": {
    "build": "snowpack build",
    "build:dist": "npm run build && electron-builder",
    "build:pack": "npm run build && electron-builder --dir",
    "format": "prettier --write src/**/*",
    "lint": "eslint --fix src/**/*",
    "start": "concurrently \"snowpack dev\" \"electron .\"",
    "test": "jest"
  },
  "dependencies": {
    "electron-is-dev": "^1.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@snowpack/app-scripts-react": "^1.4.0",
    "@snowpack/plugin-webpack": "^1.4.0",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.3",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "concurrently": "^5.2.0",
    "electron": "^9.3.1",
    "electron-builder": "^22.7.0",
    "jest": "^26.1.0",
    "postcss-cli": "^7.1.1",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "snowpack": "^2.6.3",
    "tailwindcss": "^1.4.6",
    "typescript": "^3.9.5"
  },
  "build": {
    "productName": "ElectronSnowpack",
    "asar": true,
    "files": [
      "electron",
      "build"
    ]
  }
}
